@article{parcollet_ecrire_2013,
  title = {Écrire l’histoire des expositions : réflexions sur la constitution d’un catalogue raisonné d’expositions},
  shorttitle = {Écrire l’histoire des expositions},
  author = {Parcollet, Remi and Szacka, Léa-Catherine},
  date = {2013},
  journaltitle = {Culture \& Musées},
  volume = {22},
  number = {1},
  pages = {137--162},
  doi = {10.3406/pumus.2013.1755},
  url = {https://www.persee.fr/doc/pumus_1766-2923_2013_num_22_1_1755},
  abstract = {Escribir la historia de las exposiciones : reflexiones sobre la creación de un catálogo razonado de las exposiciones. Si el objeto museal debiese ser documentado, ¿ Cómo podríamos dar cuenta del trabajo de museo en su forma más visible, es decir, la exposición ? El objeto de este artículo es interrogar la noción de catálogo, sus diferentes géneros y su evolución reciente en cuanto a la exposición. El principio de constituir el inventario más completo de la obra de un artista ó de una colección, ¿ Es aplicable a una institución productora de exposiciones ? En este contexto, el Centro Pompidou aparece como un caso de estudio ejemplar. En el marco de su programa de investigación y globalización, el instituto desarrolla, desde 2010, una reflexión sobre la historia de las exposiciones. Analizar esta iniciativa bajo la óptica de facilitar la documentación y el trabajo de catálogo de las exposiciones, enfatizando la suerte crítica que tuvieron estas iniciativas, permite tomar en cuenta la exposición como un objeto cultural en sí, un objeto a partir del cual podemos, por diversos medios, delinear la historia.},
